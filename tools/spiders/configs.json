    "rules": {
      "has": ["#thisisnot", "#auslan", "#toddslan"],
      "hasnt": []
    }
  },
  "v-alford": {
    "spider": "instagram",
    "user": "v.alford",
    "wordsRegexp": ["^([^\t\n|]+) ?||", "", 1],
    "tag": ["v.alford"],
    "rules": {
      "has": ["#wantlearnauslan"],
      "hasnt": []
    },
